<robot name ="fourleg">

<link name = "base_link">
    <visual>
        <origin rpy = "0 0 0" xyz = "0 0 0" />
        <geometry>
            <mesh filename = "./fourleg/chassis.obj" scale = "0.03 0.03 0.03" />
        </geometry>
    </visual>
    <collision>
        <origin rpy = "0 0 0" xyz = "0 0 0" />
        <geometry>
            <box size="4.35 3 1.2"/>
        </geometry>
    </collision>
