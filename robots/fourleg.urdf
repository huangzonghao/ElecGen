<?xml verison="1.0"?>
<robot name ="fourleg">

<link name = "base_link">
    <visual>
        <origin rpy = "0 0 0" xyz = "0 0 0" />
        <geometry>
            <mesh filename = "./fourleg/chassis.obj" scale = "0.01 0.01 0.01" />
        </geometry>
    </visual>
    <collision>
        <origin rpy = "0 0 0" xyz = "0 0 0" />
        <geometry>
            <box size="1.45 1 0.4 "/>
        </geometry>
    </collision>
    <inertial>
        <mass value="1"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
</link>

<link name = "link4">
