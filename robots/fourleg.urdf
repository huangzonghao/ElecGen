    </visual>
    <collision>
        <origin rpy = "0 0 0" xyz = "0 0 -0.1" />
        <geometry>
            <box size=" 0.1 0.1 0.35"/>
        </geometry>
    </collision>
    <inertial>
        <mass value="1"/>
        <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
</link>

<joint name = "base_link_link4" type = "continuous">
    <parent link = "base_link"/>
    <child link = "link4"/>
    <origin xyz = "0.35 -0.75 0" rpy = "0 0 0" />
    <axis xyz = "0 1 0" />
</joint>

